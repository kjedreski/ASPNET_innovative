            <asp:ListItem Value="Eagle Brand"></asp:ListItem>
            </asp:ListBox></td></tr>
         <tr><td><asp:Button ID="searchbtn" class="find"  Text="Search" runat="server"  OnClick="searchbtn_Click"   /></td></tr>
     </table>
    </asp:Panel>

    <asp:Label ID="output" runat="server"></asp:Label>    


 

